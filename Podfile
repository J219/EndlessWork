platform :ios, '8.0'
use_frameworks!
inhibit_all_warnings!

target 'EndlessWork' do
    
    # 底层相关
    pod 'Alamofire', '~> 4.4'               # 网络库
    pod 'FMDB'                              # 数据库
    pod 'HandyJSON', '~> 1.6.0'             # json解析
    pod 'SwiftyJSON'                        # Json对象解析
    pod 'MGJRouter', '~>0.9.0'              # 路由
    pod 'PromiseKit', '~> 4.0'              # 回调处理
    pod 'RxSwift',    '~> 3.0'              # RxSwift
    pod 'RxCocoa',    '~> 3.0'
    
    # UI相关
    pod 'MBProgressHUD', '~> 1.0.0'         #等待框
    
end





