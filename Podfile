platform :ios, '8.0'
use_frameworks!
inhibit_all_warnings!

target 'EndlessWork' do

    # 底层相关
    pod 'Alamofire', '~> 4.7'               # 网络库
    pod 'FMDB'                              # 数据库
    pod 'HandyJSON', '~> 4.1.1'             # json转模型
    pod 'SwiftyJSON'                        # Json对象解析
    pod 'MGJRouter', '~>0.9.0'              # 路由
    pod 'RxSwift',    '~> 4.0'              # RxSwift
    pod 'RxCocoa',    '~> 4.0'

    # UI相关
    pod 'MBProgressHUD', '~> 1.0.0'         # 等待框

end

target 'EndlessWorkDemo' do
    
    # 测试
    pod 'EndlessWork', :git => 'https://github.com/J219/EndlessWork.git'                       # 测试Master代码
    
end





